group1 = ["Иванов", "Петров", "Сидоров", "Смирнов", "Кузнецов", "Васильев", "Морозов", "Новиков", "Алексеев", "Зайцев"]
group2 = ["Соколов", "Козлов", "Николаев", "Орлов", "Макаров", "Федоров", "Мартынов", "Егоров", "Дмитриев", "Калинин"]

team = tuple(group1[:5] + group2[:5])

print("Группа 1:", group1)
print("Группа 2:", group2)
print("Спортивная команда:", team)

print("Длина команды:", len(team))

sorted_team = sorted(team)
print("Сортированная команда:", sorted_team)

ivanov_count = team.count("Иванов")
if ivanov_count > 0:
    print("Студент Иванов входит в команду.")
    print("Количество студентов Иванов в команде:", ivanov_count)
else:
    print("Студент Иванов не входит в команду.")
